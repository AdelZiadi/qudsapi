    <!--
        Added the constraints for entity Team.
    -->
    <changeSet id="20240517224524-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="campaign_id"
                                 baseTableName="team"
                                 constraintName="fk_team__campaign_id"
