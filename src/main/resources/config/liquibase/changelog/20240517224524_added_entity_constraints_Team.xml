                                 />

        <addForeignKeyConstraint baseColumnNames="user_info_id"
                                 baseTableName="team"
                                 constraintName="fk_team__user_info_id"
                                 referencedColumnNames="id"
                                 referencedTableName="app_user"
                                 />
    </changeSet>
</databaseChangeLog>
